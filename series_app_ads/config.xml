<?xml version='1.0' encoding='utf-8'?>
<widget id="com.realdealagency.onsportfmstreaming" version="1.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name> 
onsportfmstream
     
     </name>
    <description>
onsportfmstream
    </description>
    <author email="ceo@realdealagency.com" href="http://www.realdealagency.com">
 
onsportfmstream


    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/android/icon.png" />
        <icon density="mdpi" src="res/android/icon.png" />
        <icon density="hdpi" src="res/android/icon.png" />
        <icon density="xhdpi" src="res/android/icon.png" />
        <icon density="xxhdpi" src="res/android/icon.png" />
        <icon density="xxxhdpi" src="res/android/icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="26" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <engine name="android" spec="~6.3.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.1-dev" />
</widget>
